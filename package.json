  "main": "index.js",
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  },
  "scripts": {
    "crateseeder": "npx sequelize-cli seed:generate --name demo-user",
    "executeseeder": "npx sequelize-cli db:seed:all",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build/src/ ./src/ -s",
    "build": "npm run build-babel",
    "npm run start-And-migrate": "npm run migrate && npm start",
    "npm run start-And-unmigration": "npm run undomigrate && npm start",
    "start": "nodemon --exec babel-node ./src/index.js",
    "undomigrate": "npx sequelize-cli db:migrate:undo",
    "migrate": "sequelize db:migrate"
  },
